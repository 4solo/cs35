                        var rand = Math.random();
                        if (rand < 0.1) {
                            ad1.style.display = "block";
                        }
                        else if (rand < 0.2) {
                            ad2.style.display = "block";
                        }
                        else if (rand < 0.3) {
                            ad3.style.display = "block";
                        }
                        else if (rand < 0.4) {
                            ad4.style.display = "block";
                        }
                        else {
                            ad5.style.display = "block";
                        }
                    </script>
                </div>
            </div>
        </div>
        <div id="RightBottomEle" class="right_down_corner">
            <div class="go_learn">
                <a href="/Default.aspx" title="马上学习"></a>
            </div>
            <div class="to_top">
                <a href="javascript:ToTop();" title="回到顶部"></a>
            </div>
        </div>
        <script type="text/javascript">
            (function () {
                $backToTopFun = function () {
                    var st = $(document).scrollTop();
                    if (st > 0) {
                        $("#RightBottomEle").show();
                    }
                    else {
                        $("#RightBottomEle").hide();
                    }

                    if (st > 137) {
                        $("#RightEle").css("top", st - 137);
                    }
                    else {
                        $("#RightEle").css("top", 0);
                    }
                };
                $(window).bind("scroll", $backToTopFun);
                $(function () { $backToTopFun(); });
            })();
        </script>
    </div>

    </div>
    <div class="clear">
    </div>
