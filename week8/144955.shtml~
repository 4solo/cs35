        function ClickWebLoginButton() {
            window.location = "/login/login2.aspx?url=" + encodeURIComponent(window.location.href);
        }

        function ClickWebSignoutButton() {
            $.cookie("CookieUserName", "", { expires: 0, path: '/' });
            WebUserName.style.display = "none";
            WebLoginButton.style.display = "block";
            WebSignoutButton.style.display = "none";
        }

        function ClickCloseStudy() {
            master_study.style.display = "none";
            $.cookie("CookieStudyCourse", "", { expires: 0, path: '/' });
        }

        function ClickChildButton() {
            var course = $.cookie("CookieChildCourse");
            if (course == null || course.length == 0) {
                window.location = "/children/index.aspx";
            }
            else {
                var pos = course.indexOf(";");
                var id = course.substr(0, pos);
                window.location = "/child.aspx?CourseID=" + id;
            }
        }
    </script>
